msgstr "Ein Datum im ‚mittleren‘ Format. (%date)"
msgid "Long format"
msgstr "Langes Format"
msgid "A date in 'long' format. (%date)"
msgstr "Ein Datum im ‚langen‘ Format. (%date)"
