msgstr "<abbr title=\"Abkürzung\">Abk.</abbr>"
msgid "<acronym title=\"Three-Letter Acronym\">TLA</acronym>"
msgstr "<acronym title=\"Elektronische Datenverarbeitung\">EDV</acronym>"
msgid "Block quoted"
msgstr "Blockzitat"
